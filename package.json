{
    "name": "gps-semver",
    "displayName": "GPS.Semver",
    "description": "Updates any semver version numbers in the open document of the editor.",
    "version": "0.0.1",
    "publisher": "GatewayProgrammingSchoolInc",
    "engines": {
        "vscode": "^1.27.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:gpsSemver.bumpVersion"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "gpsSemver.bumpVersion",
                "title": "GPS: Bump Version."
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "bump-regex": "^3.1.1",
        "semver-regex": "^2.0.0"
    }
}
